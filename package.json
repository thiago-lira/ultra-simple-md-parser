{
  "name": "ultra-simple-md-parser",
  "version": "1.0.0",
  "description": "A simple markdown parser",
  "main": "dist/main.js",
  "scripts": {
    "lint": "eslint src/**/*.js",
    "build": "rm -rf dist && webpack --color --progress",
    "test": "mocha tests/**/*.spec.js --colors --require @babel/register",
    "test:watch": "yarn test --watch",
    "coverage": "nyc yarn test"
  },
  "keywords": [
    "markdown",
    "md",
    "js"
  ],
  "files": [
    "dist"
  ],
  "author": "Thiago Lira <lira.thiago@ymail.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/thiago-lira/ultra-simple-md-parser.git"
  },
  "devDependencies": {
    "@babel/core": "^7.12.10",
    "@babel/preset-env": "^7.12.11",
    "@babel/register": "^7.12.10",
    "babel-loader": "^8.2.2",
    "chai": "^4.2.0",
    "eslint": "^7.16.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-plugin-import": "^2.22.1",
    "husky": "^4.3.6",
    "mocha": "^8.2.1",
    "nyc": "^15.1.0",
    "webpack": "^5.11.0",
    "webpack-cli": "^4.2.0"
  },
  "husky": {
    "hooks": {
      "prepush": "yarn coverage",
      "precommit": "yarn eslint"
    }
  }
}
